<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <style>
        body{background-color: aqua; font-family: Verdana, Geneva, Tahoma, sans-serif; color: #ffffff; padding: 30px;}
        h1{font-size: 48px; text-transform: uppercase; letter-spacing: 2px; text-align: center;}
        p{font-size: 16px; text-align: center;}
        ul{font-size: 16px;}
    </style>
    <title>Profile</title>
</head>
<body>
    <%- include('partials/nav'); %>
    <h1>Profile of <%= person %></h1>
    <p><strong>Gender: </strong><%= data.gender %></p>
    <p><strong>Age: </strong><%= data.age %></p>
    <ul>
        <% data.hobbies.forEach(function(hobby){ %>
            <li><%= hobby %></li>
        <% }); %>
    </ul>
</body>
</html>